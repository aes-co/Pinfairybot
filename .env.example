# Telegram API Credentials (Required)
API_ID=your_telegram_api_id
API_HASH=your_telegram_api_hash
BOT_TOKEN=your_bot_token

# Bot Admin User IDs (Required for admin features)
# Comma-separated list of user IDs
ADMIN_IDS=your_admin_id1,your_admin_id2

# Browserless.io API Token (Required for Replit deployment)
# Get a free token from https://www.browserless.io/
BROWSERLESS_TOKEN=your_browserless_token

# Force Subscription Channel (Optional)
FORCE_SUB_CHANNEL=
